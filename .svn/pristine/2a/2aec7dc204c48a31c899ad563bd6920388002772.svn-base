.layout-h {
 height:2000px;
}
.layout-h .row .txt {
 width:208px;
}
.layout-h .row .rl{
 width:110px;
}
.panel-noscroll .l-footer {
 height:58px;
}
.dg-list .img{
 width: 43px;height: 38px;border-radius:100px;border: 0;margin-right: 5px;float: left;overflow:hidden;
}
.dg-list .list-image{
 width: 38px;height: 38px;border: 0;margin-right: 5px;float: left;
}
.dg-list .list-header{
 font-weight: bold; margin-bottom:5px;
}
.dg-list .list-content{
 text-overflow: ellipsis;overflow: hidden;
}
.dg-list .highlight{
 color:#000000;
}
.dg-list .dark{
 color:#808080;
}
.dg-list .rl {
 float:left;text-align:right; padding-right:10px;
}
.dg-list .abtn_r {
 cursor:pointer;color:#146cf7;margin-right:15px;float:right;
}

.mmc img {
