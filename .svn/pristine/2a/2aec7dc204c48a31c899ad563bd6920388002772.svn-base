.tal {
 text-align:left; line-height: 30px;
}
.tt-inner{
 display:inline-block;line-height:12px;padding-top:5px;
}
.infoT{
 width:100%;
}
.infoT td{
 padding:3px 5px;
}
.infoT .txtm{
 width:80%;padding: 3px 4px 3px 4px; height:20px;
}
.row .txtm{
 width:88%;padding: 3px 4px 3px 4px; height:18px;
}
.txtm {
 position: relative;border: 1px solid #ddd;background-color: #fff;vertical-align: middle;display: inline-block;overflow: hidden;
 white-space: nowrap;margin: 0;padding: 0;-moz-border-radius: 0px 0px 0px 0px;-webkit-border-radius: 0px 0px 0px 0px;border-radius: 0px 0px 0px 0px;
}
.txt-s {
 position: relative;border: 1px solid #ddd;background-color: #fff;vertical-align: middle;display: inline-block;overflow: hidden;width:100px;height:22px;
 white-space: nowrap;margin: 0;padding: 0;-moz-border-radius: 0px 0px 0px 0px;-webkit-border-radius: 0px 0px 0px 0px;border-radius: 0px 0px 0px 0px;
}

